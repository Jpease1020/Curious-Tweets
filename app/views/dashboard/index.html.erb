

<div class='row'>  <!-- choose a different month to view salses data section -->
  <div class='col-sm-3 col-md-3 col-lg-3'>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Panel title</h3>
      </div>
      <div class="panel-body">
        <ul>
          <li><%= link_to "My Profile", profile_path %></li>
          <li><%= @user.name %></li>
          <li>Screen Name: <%= @user['screen_name'] %></li>
          <li>Followers: <%= @user['followers_count'] %></li>
          <li>Following: <%= @user['friends_count'] %></li>
          <li>Tweets: <%= @user["statuses_count"] %></li>
        </ul>
      </div>
    </div>
  </div>


</div>
